          },
          {
            "key": "Exchange Set Type",
            "value": "Base"
          },
          {
            "key": "Media Type",
            "value": "DVD"
          }
        ],
        "acl": {
          "readUsers": [],
          "readGroups": [
            "distributors",
            "vars"
          ]
        },
        "expiryDate": "$(now.AddDays(21))",
